6}},{"generated":{"line":133,"column":0},"source":"js/home.js","original":{"line":78,"column":5}},{"generated":{"line":133,"column":5},"source":"js/home.js","original":{"line":76,"column":4}},{"generated":{"line":134,"column":0},"source":"js/home.js","original":{"line":79,"column":3}},{"generated":{"line":134,"column":3},"source":"js/home.js","original":{"line":62,"column":12}},{"generated":{"line":134,"column":5},"source":"js/home.js","original":{"line":79,"column":5}},{"generated":{"line":134,"column":8},"source":"js/home.js","original":{"line":62,"column":12}},{"generated":{"line":134,"column":9},"source":"js/home.js","original":{"line":62,"column":2}},{"generated":{"line":135,"column":0},"source":"js/home.js","original":{"line":80,"column":1}}],"sources":{"js/home.js":"import * as Api from './common/api.js';\nimport { getImageUrl } from './common/aws-s3.js';\nimport { randomId } from './common/useful-functions.js';\n\n// 요소(element), input 혹은 상수\nconst sliderDiv = document.querySelector('#slider');\nconst sliderArrowLeft = document.querySelector('#sliderArrowLeft');\nconst sliderArrowRight = document.querySelector('#sliderArrowRight');\n\naddAllEvents();\naddAllElements();\n\n// 여러 개의 addEventListener들을 묶어주어서 코드를 깔끔하게 하는 역할임.\nfunction addAllEvents() {}\n\n// html에 요소를 추가하는 함수들을 묶어주어서 코드를 깔끔하게 하는 역할임.\nasync function addAllElements() {\n  await addImageCardsToSlider();\n  attachSlider();\n}\n\n// api에서 카테고리 정보 및 사진 가져와서 슬라이드 카드로 사용\nasync function addImageCardsToSlider() {\n  const categorys = await Api.get('/api/categorylist');\n\n  categorys.forEach(async (category) => {\n    // 객체 destructuring\n    const { title, description, themeClass, imageKey } = category;\n    const imageUrl = await getImageUrl(imageKey);\n\n    const random = randomId();\n\n    sliderDiv.innerHTML += `\n      <div class=\"card\" id=\"a${random}\">\n        <div class=\"notification ${themeClass}\">\n          <p class=\"title is-3 is-spaced\">${title}</p>\n          <p class=\"subtitle is-6\">${description}</p>\n        </div>\n        <div class=\"card-image\">\n          <figure class=\"image is-3by2\">\n            <img\n              src=\"${imageUrl}\"\n              alt=\"카테고리 이미지\"\n            />\n          </figure>\n        </div>\n      </div>\n    `;\n\n    // 카드 클릭 시 해당 카테고리 페이지로 이동 (timeout 약간 줌, 아래와 동일 이유)\n    setTimeout(() => {\n      const card = document.querySelector(`#a${random}`);\n      card.addEventListener('click', () => {\n        window.location.href = `/products?category=${title}`;\n      });\n    }, 100);\n  });\n}\n\n// 슬라이드 가동 (위 카드가 html에 삽입된 이후에 실행되도록, timeout 약간 줌)\nfunction attachSlider() {\n  setTimeout(() => {\n    const imageSlider = bulmaCarousel.attach('#slider', {\n      //autoplay: true,\n      autoplaySpeed: 5000,\n      infinite: true,\n      duration: 600,\n      pauseOnHover: false,\n      navigation: false,\n    });\n\n    sliderArrowLeft.addEventListener('click', () => {\n      imageSlider[0].previous();\n    });\n\n    sliderArrowRight.addEventListener('click', () => {\n      imageSlider[0].next();\n    });\n  }, 200);\n}\n"},"lineCount":null}},"error":null,"hash":"9584e83c6584b09409b93e770ac16c76","cacheData":{"env":{}}}