e":78,"column":9}},{"generated":{"line":128,"column":15},"source":"js/common/useful-functions.js","original":{"line":78,"column":2}},{"generated":{"line":129,"column":0},"source":"js/common/useful-functions.js","original":{"line":79,"column":1}},{"generated":{"line":129,"column":1},"source":"js/common/useful-functions.js","original":{"line":74,"column":7}}],"sources":{"js/common/useful-functions.js":"// 문자열+숫자로 이루어진 랜덤 5글자 반환\nexport const randomId = () => {\n  return Math.random().toString(36).substring(2, 7);\n};\n\n// 이메일 형식인지 확인 (true 혹은 false 반환)\nexport const validateEmail = (email) => {\n  return String(email)\n    .toLowerCase()\n    .match(\n      /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/\n    );\n};\n\n// 주소창의 url로부터 params를 얻어 객체로 만듦\nexport const getUrlParams = () => {\n  const queryString = window.location.search;\n  const urlParams = new URLSearchParams(queryString);\n\n  const result = {};\n\n  for (const [key, value] of urlParams) {\n    result[key] = value;\n  }\n\n  return result;\n};\n\n// 숫자에 쉼표를 추가함. (10000 -> 10,000)\nexport const numberWithCommas = (n) => {\n  return n.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, ',');\n};\n\n// 로그아웃(세션스토리지에서 토큰 제거)\n// 개발 단계에서는 편의상 로컬스토리지 사용\nexport const doLogout = () => {\n  localStorage.removeItem('token');\n  window.location.href = '/';\n};\n\n// 로그인 여부(토큰 존재 여부) 확인\nexport const checkLogin = () => {\n  const token = localStorage.getItem('token');\n  if (!token) {\n    // 현재 페이지의 url 주소 추출하기\n    const pathname = window.location.pathname;\n    const search = window.location.search;\n\n    // 로그인 후 다시 지금 페이지로 자동으로 돌아가도록 하기 위한 준비작업임.\n    window.location.replace(`/login?previouspage=${pathname + search}`);\n  }\n};\n\n// 해당 주소로 이동하는 콜백함수를 반환함.\n// 이벤트 핸들 함수로 쓰면 유용함\nexport const navigate = (pathname) => {\n  return function () {\n    window.location.href = pathname;\n  };\n};\n\n// 13,000원, 2개 등의 문자열에서 쉼표, 글자 등 제외 후 숫자만 뺴냄\n// 예시: 13,000원 -> 13000, 20,000개 -> 20000\nexport const getNumbers = (string) => {\n  return parseInt(string.replace(/(,|개|원)/g, ''));\n};\n\n// ms만큼 기다리게 함.\nexport const wait = (ms) => {\n  return new Promise((r) => setTimeout(r, ms));\n};\n\n// 긴 문자열에서 뒷부분을 ...으로 바꿈\nexport const compressString = (string) => {\n  if (string.length > 10) {\n    return string.substring(0, 9) + '..';\n  }\n  return string;\n};\n"},"lineCount":null}},"error":null,"hash":"6ea5fa1b50fda85d6547f229f0484936","cacheData":{"env":{}}}